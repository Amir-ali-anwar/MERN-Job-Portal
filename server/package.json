{
  "name": "MERN-JOB-PORTAL",
  "version": "1.0.0",
  "description": "This is the project build with MERN stack having lot of cool features to help the people to keep track of their job applications.",
  "main": "app.js",
  "type": "module",
 "scripts": {
    "install-server": "npm install --prefix server",
    "install-client": "npm install --prefix client",
    "install-dependencies": "npm run install-client && npm run install-server",
    "server": "nodemon --watch server server/app.js",
    "client": "npm start --prefix client",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Amir-ali-anwar/MERN-JOB-PORTAL.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Amir-ali-anwar/MERN-JOB-PORTAL/issues"
  },
  "homepage": "https://github.com/Amir-ali-anwar/MERN-JOB-PORTAL#readme",
  "devDependencies": {
    "nodemon": "^2.0.19"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mailgen": "^2.0.27",
    "moment": "^2.29.4",
    "mongodb-memory-server": "^8.11.5",
    "mongoose": "^6.4.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "otp-generator": "^4.0.1",
    "validator": "^13.7.0"
  }
}
